generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  firstName String
  username  String @unique
  email     String @unique
  password  String
  avatarId  Int
  userPoints Int @default(1)
  lastLevel Int @default(1)
} 

