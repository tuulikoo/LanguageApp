<kw name="Evaluate JavaScript" source="/opt/homebrew/lib/python3.11/site-packages/Browser/keywords/evaluation.py" lineno="28">
<arguments repr="selector: str | None = None, *function: str, arg: Any = None, all_elements: bool = False">
<arg kind="POSITIONAL_OR_NAMED" required="false" repr="selector: str | None = None">
<name>selector</name>
<type name="Union" union="true">str | None<type name="str" typedoc="string">str</type><type name="None" typedoc="None">None</type></type>
<default>None</default>
</arg>
<arg kind="VAR_POSITIONAL" required="false" repr="*function: str">
<name>function</name>
<type name="str" typedoc="string">str</type>
</arg>
<arg kind="NAMED_ONLY" required="false" repr="arg: Any = None">
