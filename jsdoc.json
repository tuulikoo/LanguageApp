{
    "source": {
        "include": [
            "src/pages",
            "src/components",
            "src/utils",
            "src/helpers"
        ],
        "exclude": [
            "node_modules"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true
    }
}
